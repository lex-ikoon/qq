                    pane music_parm -a 0 -l 3 -H /obj/ -T OBJ -m parmeditor
                }
                tabs 0 1 {
                    pane music_net -a 0 -l 3 -H /obj/ -T OBJ -m neteditor
                }
            }
        }
    }
